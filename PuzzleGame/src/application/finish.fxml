            <Font name="Indie Flower" size="18.0" />
         </font>
      </Button>
      <Button fx:id="restart" layoutX="333.0" layoutY="211.0" mnemonicParsing="false" onAction="#Restart" prefHeight="45.0" prefWidth="105.0" style="-fx-background-color: #ffe4e1;" text="Restart" textAlignment="CENTER">
